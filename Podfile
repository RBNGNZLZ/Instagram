# platform :ios, '9.0'

target 'Instagram' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Instagram
   pod 'Parse'
   pod 'AlamofireImage'

  target 'InstagramTests' do
    inherit! :search_paths
    # Pods for testing
   pod 'Parse'
   pod 'AlamofireImage'
  end

  target 'InstagramUITests' do
    # Pods for testing
   pod 'Parse'
   pod 'AlamofireImage'
  end

end
